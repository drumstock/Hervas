<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.local.conoce_hervs">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".fragments.Home"
        android:label="@string/info"></activity>
        <activity android:name=".fragments.Informacion"
            android:label="@string/info"></activity>
        <activity android:name=".fragments.PuntosInteres"
            android:label="@string/interes"></activity>
        <activity android:name=".fragments.Alojamiento"
            android:label="@string/alojamiento"></activity>
        <activity android:name=".fragments.Restauracion"
            android:label="@string/rest"></activity>
        <activity android:name=".fragments.Rutas"
            android:label="@string/rutas"></activity>
        <activity android:name=".fragments.ComoLlegar"
            android:label="@string/llegar"></activity>



    </application>

</manifest>